{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "https://schema.org/",
    "brando": "https://brandoschema.com/",
    "dcterms": "http://purl.org/dc/terms/",
    "vann": "http://purl.org/vocab/vann/",
    "voaf": "http://purl.org/vocommons/voaf#",
    "cc": "http://creativecommons.org/ns#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "gs1": "https://ref.gs1.org/voc/",
    "risko": "https://risko.global/vocab#"
  },
  "@id": "https://risko.global/vocab",
  "@type": ["owl:Ontology", "voaf:Vocabulary"],
  "dcterms:title": "Risko: Integrity Risk Vocabulary v1.1",
  "dcterms:description": "A vocabulary for modelling organisational integrity, country/sector/political risk, counterparties, controls, incidents, investigations, enforcement actions, and scenario-based overrides.",
  "dcterms:creator": "YOUR_ORG_NAME",
  "dcterms:license": "https://creativecommons.org/licenses/by/4.0/",
  "vann:preferredNamespacePrefix": "risko",
  "vann:preferredNamespaceUri": "https://risko.global/vocab#",
  "@graph": [

    /* =========================
       CORE CLASSES
       ========================= */

    {
      "@id": "risko:Entity",
      "@type": "owl:Class",
      "rdfs:label": "Risko Entity",
      "rdfs:comment": "Base class for all integrity-risk-related resources.",
      "rdfs:subClassOf": { "@id": "schema:Thing" }
    },
    {
      "@id": "risko:Organization",
      "@type": "owl:Class",
      "rdfs:label": "Organization",
      "rdfs:comment": "An organization whose integrity risk position is being assessed.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Organization" }
      ]
    },
    {
      "@id": "risko:OrgUnit",
      "@type": "owl:Class",
      "rdfs:label": "Organizational Unit",
      "rdfs:comment": "A business unit or subsidiary within an integrity-scoped organization.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Organization" }
      ]
    },
    {
      "@id": "risko:Counterparty",
      "@type": "owl:Class",
      "rdfs:label": "Counterparty",
      "rdfs:comment": "Any external party (supplier, agent, distributor, JV partner, service provider) that can carry integrity risk.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Organization" }
      ]
    },
    {
      "@id": "risko:Person",
      "@type": "owl:Class",
      "rdfs:label": "Person",
      "rdfs:comment": "A natural person relevant to the integrity risk position (employee, director, owner, official).",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Person" }
      ]
    },
    {
      "@id": "risko:PEP",
      "@type": "owl:Class",
      "rdfs:label": "Politically Exposed Person",
      "rdfs:comment": "A person holding or having held a prominent public function and treated as a politically exposed person.",
      "rdfs:subClassOf": { "@id": "risko:Person" }
    },
    {
      "@id": "risko:Role",
      "@type": "owl:Class",
      "rdfs:label": "Role",
      "rdfs:comment": "A role played by a person in an organization or counterparty that is relevant for integrity risk.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Role" }
      ]
    },
    {
      "@id": "risko:Jurisdiction",
      "@type": "owl:Class",
      "rdfs:label": "Jurisdiction",
      "rdfs:comment": "A country or administrative area with a distinct legal and political regime.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:AdministrativeArea" }
      ]
    },
    {
      "@id": "risko:Sector",
      "@type": "owl:Class",
      "rdfs:label": "Sector",
      "rdfs:comment": "An industry or sector classification relevant for integrity risk analysis.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "skos:Concept" }
      ]
    },
    {
      "@id": "risko:Location",
      "@type": "owl:Class",
      "rdfs:label": "Location",
      "rdfs:comment": "A physical location such as a site, office, plant, or project location.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Place" }
      ]
    },
    {
      "@id": "risko:TimePeriod",
      "@type": "owl:Class",
      "rdfs:label": "Time Period",
      "rdfs:comment": "A bounded time period such as a fiscal year, quarter, or assessment window.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:RiskDimension",
      "@type": "owl:Class",
      "rdfs:label": "Risk Dimension",
      "rdfs:comment": "A dimension of risk (e.g. corruption, sanctions, fraud, political violence) that can be measured.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "skos:Concept" }
      ]
    },
    {
      "@id": "risko:RiskIndicator",
      "@type": "owl:Class",
      "rdfs:label": "Risk Indicator",
      "rdfs:comment": "A measured indicator for a specific risk dimension, such as a score or rating.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:RiskProfile",
      "@type": "owl:Class",
      "rdfs:label": "Risk Profile",
      "rdfs:comment": "A collection of risk indicators and ratings describing the risk position for a given context.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Intangible" }
      ]
    },
    {
      "@id": "risko:CountryRiskProfile",
      "@type": "owl:Class",
      "rdfs:label": "Country Risk Profile",
      "rdfs:comment": "Risk profile for a specific jurisdiction (country or region).",
      "rdfs:subClassOf": { "@id": "risko:RiskProfile" }
    },
    {
      "@id": "risko:SectorRiskProfile",
      "@type": "owl:Class",
      "rdfs:label": "Sector Risk Profile",
      "rdfs:comment": "Risk profile for a specific sector, optionally in a given jurisdiction.",
      "rdfs:subClassOf": { "@id": "risko:RiskProfile" }
    },
    {
      "@id": "risko:PoliticalRiskProfile",
      "@type": "owl:Class",
      "rdfs:label": "Political Risk Profile",
      "rdfs:comment": "Risk profile capturing political risk at country, sector, organization, or counterparty level.",
      "rdfs:subClassOf": { "@id": "risko:RiskProfile" }
    },
    {
      "@id": "risko:Risk",
      "@type": "owl:Class",
      "rdfs:label": "Integrity Risk",
      "rdfs:comment": "A specific integrity risk such as bribery, fraud, sanctions, conflicts of interest, human rights, or data privacy.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Intangible" }
      ]
    },
    {
      "@id": "risko:RiskFactor",
      "@type": "owl:Class",
      "rdfs:label": "Risk Factor",
      "rdfs:comment": "A factor that drives or contributes to a specific integrity risk.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:RiskScore",
      "@type": "owl:Class",
      "rdfs:label": "Risk Score",
      "rdfs:comment": "A numeric or categorical score assigned to a risk.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:RiskAssessment",
      "@type": "owl:Class",
      "rdfs:label": "Risk Assessment",
      "rdfs:comment": "An assessment of integrity risks for a given organization, unit, or counterparty.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Action" }
      ]
    },
    {
      "@id": "risko:Policy",
      "@type": "owl:Class",
      "rdfs:label": "Integrity Policy",
      "rdfs:comment": "A policy addressing one or more integrity risk categories (e.g. anti-bribery, sanctions, fraud).",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:CreativeWork" }
      ]
    },
    {
      "@id": "risko:Control",
      "@type": "owl:Class",
      "rdfs:label": "Integrity Control",
      "rdfs:comment": "A specific control designed to prevent, detect, or correct integrity risks.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:AssuranceActivity",
      "@type": "owl:Class",
      "rdfs:label": "Assurance Activity",
      "rdfs:comment": "An internal or external assurance activity such as an audit, review, or certification.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Action" }
      ]
    },
    {
      "@id": "risko:Incident",
      "@type": "owl:Class",
      "rdfs:label": "Integrity Incident",
      "rdfs:comment": "An alleged or confirmed integrity breach such as bribery, fraud, misreporting, harassment, or sanctions violation.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Event" }
      ]
    },
    {
      "@id": "risko:WhistleblowingReport",
      "@type": "owl:Class",
      "rdfs:label": "Whistleblowing Report",
      "rdfs:comment": "A report raising concerns about suspected integrity issues.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:Investigation",
      "@type": "owl:Class",
      "rdfs:label": "Investigation",
      "rdfs:comment": "A forensic or compliance investigation into one or more incidents.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Action" }
      ]
    },
    {
      "@id": "risko:CorrectiveAction",
      "@type": "owl:Class",
      "rdfs:label": "Corrective Action",
      "rdfs:comment": "A remediation or improvement action taken in response to identified integrity issues.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:EnforcementAction",
      "@type": "owl:Class",
      "rdfs:label": "Enforcement Action",
      "rdfs:comment": "An action by a regulator, prosecutor, or court in response to integrity breaches (fine, settlement, debarment, etc.).",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:Event" }
      ]
    },
    {
      "@id": "risko:EvidenceItem",
      "@type": "owl:Class",
      "rdfs:label": "Evidence Item",
      "rdfs:comment": "A document or artefact used as evidence in risk assessments, incidents, or investigations.",
      "rdfs:subClassOf": [
        { "@id": "risko:Entity" },
        { "@id": "schema:CreativeWork" }
      ]
    },
    {
      "@id": "risko:RiskDataSource",
      "@type": "owl:Class",
      "rdfs:label": "Risk Data Source",
      "rdfs:comment": "A source of risk-relevant data such as external ratings, internal models, or regulator data.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },

    /* === NEW IN v1.1: Scenario + Overrides === */

    {
      "@id": "risko:Scenario",
      "@type": "owl:Class",
      "rdfs:label": "Scenario",
      "rdfs:comment": "A specific contextual situation (country, sector, time, trigger) in which risk and control behaviour may differ.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:ScenarioOverride",
      "@type": "owl:Class",
      "rdfs:label": "Scenario Override",
      "rdfs:comment": "An override that adjusts risk profiles, risks, policies or controls when a given scenario applies.",
      "rdfs:subClassOf": { "@id": "risko:Entity" }
    },

    /* =========================
       KEY PROPERTIES
       ========================= */

    /* Generic metadata */

    {
      "@id": "risko:identifier",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Identifier",
      "rdfs:comment": "Local identifier for the resource.",
      "rdfs:domain": { "@id": "risko:Entity" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:externalId",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "External ID",
      "rdfs:comment": "Identifier from an external system (GRC tool, ERP, case system).",
      "rdfs:domain": { "@id": "risko:Entity" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:status",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Status",
      "rdfs:comment": "Lifecycle status of the resource (Active, Inactive, Draft, Closed, etc.).",
      "rdfs:domain": { "@id": "risko:Entity" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:createdAt",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Created At",
      "rdfs:comment": "Creation timestamp.",
      "rdfs:domain": { "@id": "risko:Entity" },
      "rdfs:range": { "@id": "xsd:dateTime" }
    },
    {
      "@id": "risko:updatedAt",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "Updated At",
      "rdfs:comment": "Last update timestamp.",
      "rdfs:domain": { "@id": "risko:Entity" },
      "rdfs:range": { "@id": "xsd:dateTime" }
    },

    /* Org & structure */

    {
      "@id": "risko:hasOrgUnit",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has organizational unit",
      "rdfs:domain": { "@id": "risko:Organization" },
      "rdfs:range": { "@id": "risko:OrgUnit" }
    },
    {
      "@id": "risko:partOfOrganization",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "part of organization",
      "rdfs:domain": { "@id": "risko:OrgUnit" },
      "rdfs:range": { "@id": "risko:Organization" }
    },
    {
      "@id": "risko:operatesInJurisdiction",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "operates in jurisdiction",
      "rdfs:domain": { "@id": "risko:Organization" },
      "rdfs:range": { "@id": "risko:Jurisdiction" }
    },
    {
      "@id": "risko:activeInSector",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "active in sector",
      "rdfs:domain": { "@id": "risko:Organization" },
      "rdfs:range": { "@id": "risko:Sector" }
    },
    {
      "@id": "risko:hasCounterparty",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has counterparty",
      "rdfs:domain": { "@id": "risko:Organization" },
      "rdfs:range": { "@id": "risko:Counterparty" }
    },

    /* Counterparties & people */

    {
      "@id": "risko:counterpartyOf",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "counterparty of",
      "rdfs:domain": { "@id": "risko:Counterparty" },
      "rdfs:range": { "@id": "risko:Organization" }
    },
    {
      "@id": "risko:counterpartyType",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "counterparty type",
      "rdfs:comment": "Type of counterparty (Supplier, Agent, Distributor, JV Partner, Service Provider).",
      "rdfs:domain": { "@id": "risko:Counterparty" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:locatedInJurisdiction",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "located in jurisdiction",
      "rdfs:domain": { "@id": "risko:Counterparty" },
      "rdfs:range": { "@id": "risko:Jurisdiction" }
    },
    {
      "@id": "risko:hasRole",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has role",
      "rdfs:domain": { "@id": "risko:Person" },
      "rdfs:range": { "@id": "risko:Role" }
    },
    {
      "@id": "risko:roleHolder",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "role holder",
      "rdfs:domain": { "@id": "risko:Role" },
      "rdfs:range": { "@id": "risko:Person" }
    },
    {
      "@id": "risko:roleInOrganization",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "role in organization",
      "rdfs:domain": { "@id": "risko:Role" },
      "rdfs:range": { "@id": "risko:Organization" }
    },
    {
      "@id": "risko:roleType",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "role type",
      "rdfs:comment": "Role type label (e.g. BoardMember, CountryManager, ThirdPartyAgent).",
      "rdfs:domain": { "@id": "risko:Role" },
      "rdfs:range": { "@id": "xsd:string" }
    },

    /* Jurisdiction & sector */

    {
      "@id": "risko:isoCountryCode",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "ISO country code",
      "rdfs:domain": { "@id": "risko:Jurisdiction" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:hasCountryRiskProfile",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has country risk profile",
      "rdfs:domain": { "@id": "risko:Jurisdiction" },
      "rdfs:range": { "@id": "risko:CountryRiskProfile" }
    },
    {
      "@id": "risko:hasSectorRiskProfile",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has sector risk profile",
      "rdfs:domain": { "@id": "risko:Sector" },
      "rdfs:range": { "@id": "risko:SectorRiskProfile" }
    },

    /* Risk dimensions & indicators */

    {
      "@id": "risko:dimensionCode",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "dimension code",
      "rdfs:domain": { "@id": "risko:RiskDimension" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:forDimension",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "for dimension",
      "rdfs:domain": { "@id": "risko:RiskIndicator" },
      "rdfs:range": { "@id": "risko:RiskDimension" }
    },
    {
      "@id": "risko:value",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "indicator value",
      "rdfs:comment": "Value of a risk indicator (numeric or categorical encoded as string).",
      "rdfs:domain": { "@id": "risko:RiskIndicator" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:scale",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "scale",
      "rdfs:comment": "Description of the indicator scale (e.g. 0–100, 1–5, Low/Medium/High).",
      "rdfs:domain": { "@id": "risko:RiskIndicator" },
      "rdfs:range": { "@id": "xsd:string" }
    },

    /* Risk profiles */

    {
      "@id": "risko:overallRiskRating",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "overall risk rating",
      "rdfs:domain": { "@id": "risko:RiskProfile" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:overallRiskScore",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "overall risk score",
      "rdfs:domain": { "@id": "risko:RiskProfile" },
      "rdfs:range": { "@id": "xsd:decimal" }
    },
    {
      "@id": "risko:hasRiskIndicator",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has risk indicator",
      "rdfs:domain": { "@id": "risko:RiskProfile" },
      "rdfs:range": { "@id": "risko:RiskIndicator" }
    },
    {
      "@id": "risko:forJurisdiction",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "for jurisdiction",
      "rdfs:domain": { "@id": "risko:CountryRiskProfile" },
      "rdfs:range": { "@id": "risko:Jurisdiction" }
    },
    {
      "@id": "risko:forSector",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "for sector",
      "rdfs:domain": { "@id": "risko:SectorRiskProfile" },
      "rdfs:range": { "@id": "risko:Sector" }
    },

    /* Risks & scores */

    {
      "@id": "risko:riskCategory",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "risk category",
      "rdfs:comment": "High-level risk category (Bribery, Fraud, Sanctions, etc.).",
      "rdfs:domain": { "@id": "risko:Risk" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:riskDescription",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "risk description",
      "rdfs:domain": { "@id": "risko:Risk" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:hasRiskFactor",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has risk factor",
      "rdfs:domain": { "@id": "risko:Risk" },
      "rdfs:range": { "@id": "risko:RiskFactor" }
    },
    {
      "@id": "risko:hasRiskScore",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "has risk score",
      "rdfs:domain": { "@id": "risko:Risk" },
      "rdfs:range": { "@id": "risko:RiskScore" }
    },
    {
      "@id": "risko:scoreValue",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "score value",
      "rdfs:domain": { "@id": "risko:RiskScore" },
      "rdfs:range": { "@id": "xsd:decimal" }
    },
    {
      "@id": "risko:scoreLabel",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "score label",
      "rdfs:domain": { "@id": "risko:RiskScore" },
      "rdfs:range": { "@id": "xsd:string" }
    },

    /* Risk assessment */

    {
      "@id": "risko:assessedEntity",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "assessed entity",
      "rdfs:domain": { "@id": "risko:RiskAssessment" },
      "rdfs:range": { "@id": "risko:Entity" }
    },
    {
      "@id": "risko:assessmentDate",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "assessment date",
      "rdfs:domain": { "@id": "risko:RiskAssessment" },
      "rdfs:range": { "@id": "xsd:date" }
    },

    /* Policies & controls */

    {
      "@id": "risko:policyType",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "policy type",
      "rdfs:domain": { "@id": "risko:Policy" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:coversRiskCategory",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "covers risk category",
      "rdfs:domain": { "@id": "risko:Policy" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:controlType",
      "@type": "owl:DatatypeProperty",
      "rdfs:label": "control type",
      "rdfs:domain": { "@id": "risko:Control" },
      "rdfs:range": { "@id": "xsd:string" }
    },
    {
      "@id": "risko:mitigatesRisk",
      "@type": "owl:ObjectProperty",
      "rdfs:label": "mitigates risk",
